{
  "version": 2,
  "builds": [
    {
      "src": "api/compress.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/compress",
      "dest": "api/compress.js"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
